{
  "serverRoot": "https://kanban.openteams.me",
  "port": 8000,
  "dbtype": "postgres",
  "dbconfig": "postgres://focalboarduser:strongpassword@postgres:5432/focalboard?sslmode=disable",
  "useSSL": false,
  "enableLocalMode": false,
  "localModeSocketLocation": "/var/tmp/focalboard_local.socket",
  "enablePublicSharedBoards": true,
  "featureFlags": {},
  "session": {
    "secret": "generate-a-strong-random-secret",
    "expire_days": 30
  },
  "ldap": {
    "enabled": true,
    "server": "ldap://openldap:389",
    "baseDN": "dc=openteams,dc=me",
    "bindUsername": "cn=admin,dc=openteams,dc=me",
    "bindPassword": "your-ldap-admin-password",
    "userFilter": "(objectClass=person)",
    "firstNameAttribute": "givenName",
    "lastNameAttribute": "sn",
    "emailAttribute": "mail",
    "usernameAttribute": "uid",
    "loginIdAttribute": "uid"
  }
}